<?xml version="1.0"?>
<!-- DO NOT MODIFY APPSETTINGS.DEFAULT.CONFIG -->
<!-- It will be copied to AppSettings.config by the build. -->
<!-- Change your settings there. -->
<appSettings>
  <add key="webpages:Version" value="3.0.0.0" />
  <add key="webpages:Enabled" value="false" />
  <!--<add key="ClientValidationEnabled" value="true" />-->
  <add key="UnobtrusiveJavaScriptEnabled" value="true" />
  <add key="StorageConnectionString" value="UseDevelopmentStorage=true" />
  <!-- API KEYS -->
  <add key="SendGridUsername" value="" />
  <add key="SendGridPassword" value="" />
  <add key="XboxClientId" value="" />
  <add key="XboxClientSecret" value="" />
  <add key="FacebookClientId" value="" />
  <add key="FacebookClientSecret" value="" />
  <!-- Defines the maximum number of users allowed to register -->
  <add key="MaxUserCount" value="0" />
  <!-- Any tags you want to add to the header of the page -->
  <add key="HeaderHtml" value="" />
</appSettings>
