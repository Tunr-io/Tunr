<div id="component_Player" class="COMPONENT player">
	<ul class="frames">
		<li class="library HELPER showArtists" data-helper-class="LibraryHelper">
			<div class="uploadProgress"></div>
			<nav>
				<a class="music" href="#">Music</a>
				<a class="artist" href="#">Artist</a>
				<a class="album" href="#">Album</a>
			</nav>
			<ul class="artists"></ul>
			<ul class="albums"></ul>
			<ul class="songs"></ul>
		</li>
		<li class="playlist HELPER" data-helper-class="PlaylistHelper">
			<h1>Playlist</h1>
			<ul class="songlist"></ul>
		</li>
		<li class="playing HELPER" data-helper-class="PlayingHelper">
			<div class="title animated">
				<h1>Now Playing</h1>
				<h2>Some Artist</h2>
			</div>
			<div class="playtimer">0:00</div>
			<div class="art">
				<img src="/svg/album_cover.svg" alt="Album Art" />
			</div>
			<ul class="controls">
				<li class="prev"><button></button></li>
				<li class="pause"><button></button></li>
				<li class="play"><button></button></li>
				<li class="next"><button></button></li>
			</ul>
			<canvas></canvas>
		</li>
	</ul>
</div>