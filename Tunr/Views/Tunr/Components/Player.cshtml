<div id="component_Player" class="COMPONENT">
	<ul class="frames">
		<li class="library">
			<nav>
				<a href="#">Music</a>
			</nav>
		</li>
		<li class="playlist">
			<h1>Playlist</h1>
			<ul class="songlist"></ul>
		</li>
		<li class="playing">
			<div class="title animated">
				<h1>Now Playing</h1>
				<h2>Some Artist</h2>
			</div>
			<div class="playtimer">0:00</div>
			<div class="art">
				<img src="/svg/album_cover.svg" alt="Album Art" />
			</div>
			<ul class="controls">
				<li class="prev"><button></button></li>
				<li class="pause"><button></button></li>
				<li class="play"><button></button></li>
				<li class="next"><button></button></li>
			</ul>
			<canvas></canvas>
		</li>
	</ul>
</div>